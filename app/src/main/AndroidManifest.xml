<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.apps_covidrt">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".InformasiActivity"></activity>
        <activity android:name=".WebActivityWarga" />
        <activity android:name=".WebActivity" />
        <activity android:name=".rt.laporankesejahteraan.LaporanKesejahteraanActivity" />
        <activity android:name=".rt.laporankesehatan.LaporanKesehatanActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MenuPendaftaran" />
        <activity android:name=".SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".warga.kondisikesejahteraan.KondisiKesejahteraanActivity" />
        <activity android:name=".warga.kondisikesehatan.KondisiKesehatanActivity" />
        <activity android:name=".warga.gantipassword.GantiPasswordWargaActivity" />
        <activity android:name=".warga.editprofile.EditProfileWargaActivity" />
        <activity android:name=".warga.menuutama.MainWargaActivity" />
        <activity android:name=".warga.pendaftaran.PendaftaranWargaActivity" />
        <activity android:name=".rt.gantipassword.GantiPasswordRTActivity" />
        <activity android:name=".rt.editprofile.EditProfileRTActivity" />
        <activity android:name=".rt.pendaftaran.PendaftaranRTActivity" />
        <activity android:name=".rt.menuutama.MainActivity" />
    </application>

</manifest>